    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^20.8.2",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.16",
    "tailwindcss": "^3.3.3",
    "typescript": "~5.2.2",
    "vite": "^4.4.11",
    "vite-plugin-css-injected-by-js": "^3.3.0",
    "vue-tsc": "^1.8.15"
