{
  "name": "@floatie/widget-vue",
  "version": "3.1.0",
  "private": false,
  "type": "module",
  "files": [
    "dist",
    "dist/types"
  ],
  "module": "dist/index.js",
  "types": "dist/types/widget-vue/src/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "bun type-check && bun build-only && bun generate-types",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit --composite false",
    "generate-types": "rm -rf src/types && vue-tsc --emitDeclarationOnly"
  },
  "dependencies": {
    "@floatie/widget-core": "workspace:*",
    "vue": "^3.5.8"
  },
  "devDependencies": {
    "@iconify/vue": "^4.1.2",
    "@tsconfig/node18": "^18.2.4",
    "@types/node": "^22.7.2",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "typescript": "~5.6.2",
    "vite": "^5.4.8",
    "vite-plugin-css-injected-by-js": "^3.5.1",
    "vue-tsc": "^2.1.6"
  }
}