{
  "name": "@floatie/widget-vue",
  "version": "3.2.0",
  "private": false,
  "type": "module",
  "files": [
    "dist",
    "dist/types"
  ],
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "pnpm type-check && pnpm build-only && pnpm generate-types",
    "preview": "vite preview",
